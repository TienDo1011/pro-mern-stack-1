    "lodash": "^4.17.4",
    "mongodb": "^2.2.6",
    "mongoose": "^4.11.12",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.10",
    "react-transition-group": "1.2.0",
    "reactstrap": "^4.8.0",
    "source-map-support": "^0.4.2"
